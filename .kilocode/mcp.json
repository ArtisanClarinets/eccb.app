{
    "mcpServers": {
        "prisma-mcp-server": {
            "command": "npx",
            "args": [
                "-y",
                "prisma",
                "mcp"
            ],
            "env": {},
            "alwaysAllow": [
                "migrate-status",
                "migrate-dev",
                "migrate-reset",
                "Prisma-Studio"
            ]
        },
        "Better Auth": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.inkeep.com/better-auth/mcp"
            ],
            "disabledTools": [],
            "disabled": false,
            "alwaysAllow": [
                "search-better-auth-docs",
                "ask-question-about-better-auth"
            ]
        }
    }
}